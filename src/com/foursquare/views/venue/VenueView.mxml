<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					  xmlns:s="library://ns.adobe.com/flex/spark" 
					  xmlns:mx="library://ns.adobe.com/flex/mx"
					  xmlns:search="com.foursquare.views.search.*"
					  width="100%" height="100%"
					  >
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:layout>
		<s:VerticalLayout />
	</s:layout>
	
	<s:TabBar dataProvider="{myViewStack}"/> 
	
	<mx:ViewStack id="myViewStack"  width="100%" height="100%"
				  borderStyle="solid"> 
		
		<s:NavigatorContent id="search" label="Search"> 
			<search:SearchView width="100%" height="100%" /> 
		</s:NavigatorContent> 
		
		<s:NavigatorContent id="add" label="Add Venue"> 
			<s:Label text="Add Venue"/> 
			<s:Group height="150" width="100%">
				<!--style of window-->
				<s:Line width="100%" height="1" top="0">
					<s:stroke>
						<s:SolidColorStroke color="0xffffff" />
					</s:stroke>
				</s:Line>
				
				<s:Rect left="0" right="0" top="0" bottom="0">
					<s:fill>
						<s:SolidColor color="0x94bccf" />
					</s:fill>
				</s:Rect>
			</s:Group>
		</s:NavigatorContent> 
		
	</mx:ViewStack> 	
</s:SkinnableContainer>
